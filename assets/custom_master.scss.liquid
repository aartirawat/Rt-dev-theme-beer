  .qtyAdjust .adjust.minus {
    left: -20px;
    top: 7px;
  }
  .qtyAdjust input {
    height: 28px;
    line-height: 28px;
    width: 28px;
  }
  .qtyAdjust .adjust.plus {
    top: 7px;
    right: -20px;
  }
  .cart-item .qtyAdjust {
    min-width: 30px;
  }
  .cart-item--info div.cart-item--quantity {
    display: inline-block;
    max-width: 85px;
    width: 100%;
    min-width: inherit;
  }
  .cart-item--remove-link {
    text-decoration: none;
    margin-right: 15px;
    img {
      max-width: 15px;
    }
  }
  .cart-item .cart-item--total {
    float: right;
    margin-left: 5px;
  }
  .cart-item--info>div:not(.shipdate) {
    margin-top: 0;
    vertical-align: top;
  }
  .cart-item .shipdate img {
    max-height: 16px;
  }
  .hideVerySmall {
    display: none !important;
  }
}
@media(max-width:360px) {
  .site-main {
    margin-top: 97px;
  }
  .instagram--photo {
    width: 145px;
  }
}