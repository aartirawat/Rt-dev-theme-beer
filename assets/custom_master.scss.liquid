.slick-arrow {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
  background: none;
  border: none;
  width: 30px;
  height: 30px;
  padding: 0;
  z-index: 2;
  cursor: pointer;
  &::after {
    position: absolute;
    border-top: 1px solid;
    border-left: 1px solid;
    height: 20px;
    width: 20px;
    content: ' ';
    top: 5px;
  }
}
.slick-prev {
  top: 5px;
  &::after {
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    left: 5px;
  }
}
.slick-next {
  bottom: 5px;
  &::after {
    transform: rotate(-135deg);
    -webkit-transform: rotate(-135deg);
    left: 5px;

  }
}
.slick-arrow.slick-disabled {
  opacity: 0.5;
  cursor: auto;
}
