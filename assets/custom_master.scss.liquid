.productSticky {
  .container {
    margin: 8px auto;
    position: relative;
  }

  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
  box-shadow: 1px 1px 3px #eee;
  z-index: 999;
  opacity: 0;
  visibility: hidden;
  height: 0;
  overflow: hidden;
  @include transition();

  .sticky-img {
    display: inline-block;
    vertical-align: middle;
    width: 100px;
  }

  .sticky-btn {
    display: inline-block;
    vertical-align: middle;
    width: 180px;
    text-align: right;
  }

  .sticky-content {
    display: inline-block;
    width: calc(100% - 280px);
    vertical-align: top;
    padding: 15px 20px 0;
    h3 {
      margin: 0 5px 0 0;
      display: inline;
      margin-right: 8px;
      vertical-align: middle;
      font-size: 24px;
      line-height: 1.1;
      cursor: pointer;
    }
    .TurnToReviewsTeaser {
      display: inline-block;
      margin: 0;
      vertical-align: middle;
    }
  }

  ul {
    list-style: none;
    padding: 5px 0 0;
    margin: 10px 0 0;
    position: absolute;
    bottom: 10px;
    background: #fff;
    li {
      display: inline-block;
      padding-right: 50px;
      &:last-child {
        padding: 0;
      }
      a {
        color: #333;
        text-decoration: none;
        position: relative;
        &.selected {
          color: $link_color;
          &:after {
            content: ' ';
            left: 0;
            border-bottom: 2px solid;
            bottom: -10px;
            width: 100%;
            position: absolute;
          }
        }
      }
    }
  }

  @media(max-width: 991px) {
    .sticky-img, .sticky-content > h3, .sticky-content > span, .product-form--atc-button svg, ul a span:not(.reviewCount) {
      display: none !important;
    }
    .product-form--atc-button {
      padding: 5px 10px;
      margin-top: 0;
      span {
        font-size: 11px;
        display: inherit;
        line-height: 20px;
      }
    }

    ul {
      top: -5px;
      li {
        padding-right: 15px;
        a {
          padding-bottom: 2px;
        }
      }
    }
    .productSticky .sticky-content {
      width: calc(100% - 180px);
      padding: 3px 00px 0;
    }
  }

  @media(max-width: 410px) {
    ul li {
      padding-right: 10px;
    }
    ul a {
      font-size: 12px;
    }
  }

  @media(max-width: 364px) {
    ul li {
      padding-right: 5px;
    }
  }

  @media(min-width: 992px) {
    ul {
      left: 250px;
    }
  }
}

