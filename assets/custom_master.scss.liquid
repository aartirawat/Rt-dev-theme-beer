@import url('https://fonts.googleapis.com/css?family=Oleo+Script:400,700');
$link_color: #CD0E2C;
{% assign font_body = settings.font_body %}
{% if font_body contains 'Google' %}
  {% assign font_parts = font_body | split: '_' %}
  {% assign font_name = font_parts[1] %}
  {% capture font_body %}"{{ font_name | split: ':' | first | replace: '+', ' ' }}", {% if font_parts.last == 'serif' %}serif{% else %}sans-serif{% endif %}{% endcapture %}
{% endif %}
$font-body: {{ font_body }};
@mixin transition() {
  transition: 0.4s all;
  -webkit-transition: 0.4s all;
}
